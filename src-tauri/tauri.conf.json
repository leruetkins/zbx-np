{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "zbx-np-tauri",
  "version": "0.1.0",
  "identifier": "com.zbxnp.dev",
  "build": {
    "beforeDevCommand": "cargo run --bin zbx-np-server",
    "beforeBuildCommand": "cargo build --bin zbx-np-server --release",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:7000/console"
  },
  "app": {
    "windows": [
      {
        "title": "zbx-np Tauri",
        "resizable": true,
        "fullscreen": false,
        "url": "http://localhost:7000/console",
        "label": "main",
        "visible": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": []
  }
}